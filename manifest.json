{
  "manifest_version": 2,
  "name": "Contract Duplicator",
  "description": "Duplicate contracts, easily.",
  "version": "1.00",
  "content_scripts": [
    {
      "matches": [
        "*://*.worldtravelink.com/"
      ],
      "js": ["./scripts/content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons":{
    "16": "./icon.png",
    "48": "./icon.png",
    "128": "./icon.png"
  },
  "permissions": [
    "tabs"
  ]
}