{
    "manifest_version": 2,
    "name": "Contract Duplicator",
    "description": "Duplicate contracts, easily.",
    "version": "1.00",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["./scripts/content.js"]
      }
    ],
    "permissions": [
      "tabs"
    ]
  }